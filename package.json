    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.3",
        "mongoose-long": "^0.3.2",
        "mongoose-paginate-v2": "^1.3.17",
        "morgan": "^1.10.0",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.15",
        "@babel/node": "^7.13.13",
        "@babel/plugin-transform-runtime": "^7.13.15",
        "@babel/preset-env": "^7.13.15",
        "eslint": "^7.24.0",
        "nodemon": "^2.0.7"
    }
}
